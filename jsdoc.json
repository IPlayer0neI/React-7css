{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "src"
    ],
    "exclude": [
      "node_modules"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs",
    "recurse": true
  }
}